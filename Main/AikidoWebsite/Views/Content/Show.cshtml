@model AikidoWebsite.Data.Entities.Seite

@{
    ViewBag.Title = Model.Name;
}

<div class="renderContent">
@Html.Raw(Model.Html)
</div>


@if (User.IsInRole("admin")) {
    <br />
    <i>Diese Seite wurde von @Model.Autor am @Model.ErstellungsDatum bearbeitet. Seite <a href="/Content/Edit?id=@Model.Name">bearbeiten</a>.</i>
}